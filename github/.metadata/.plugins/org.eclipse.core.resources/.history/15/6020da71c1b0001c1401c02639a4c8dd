package com.capgemini.day5;

public class FloatArray {
	public static void main(String[] args) {
		
		float [][]arr=new float[2][3];
		arr[0][0]=10;
		arr[0][1]=20;
		arr[0][2]=30;
		arr[1][0]=40;
		arr[1][1]=50;
		arr[1][2]=60;
		for(float[] i:arr)
		{
			for(float j:i)
			{
				System.out.print(j+ " ");
			}
			System.out.println();
		}
	}
}
